#!/usr/bin/env bash

prelude () {
    cd $WD/model/data/
    if [ ! -f data.csv ]; then
        cat $WD/sql/shots.sql | sqlite3 -csv $WD/ruskie.db > data.csv
    fi
    python ../convert.py data.csv data.libsvm
    head -n 10000 data.libsvm > train.libsvm
    tail -n+10001 data.libsvm > test.libsvm
}

prelude
